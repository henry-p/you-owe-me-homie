<h1>New Dwelling</h1>

<% if @household.errors.any? %>
  <ul>
    <% @household.errors.full_messages.each do |msg| %>
      <li> <%= msg %> </li>
    <% end %>
  </ul>
<% end %>

<%= form_tag(households_path) do %>
  <%= text_field_tag :name %>
  <%= text_field_tag :address %>
  <%= submit_tag "Create Dwelling" %>
<% end %>


